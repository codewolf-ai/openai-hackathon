# Copy to .env and set your API key
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Codex app-server integration for task execution
CODEX_APP_SERVER_CMD=/Applications/Codex.app/Contents/Resources/codex app-server
CODEX_APP_SANDBOX=workspace-write
CODEX_APP_APPROVAL_POLICY=never
CODEX_APP_MODEL=gpt-5.2-codex
CODEX_APP_TIMEOUT_MS=120000
